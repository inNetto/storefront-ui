{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt prepare && nuxt dev",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "codegen": "graphql-codegen",
    "predev": "yarn codegen",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix"
  },
  "dependencies": {
    "@apollo/client": "^3.8.6",
    "@erpgap/odoo-sdk": "^0.7.0",
    "@erpgap/odoo-sdk-api-client": "^0.7.0",
    "@kgierke/nuxt-basic-auth": "^1.4.0",
    "@nuxt/devtools": "^1.0.0",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/image": "^1.0.0",
    "@nuxtjs/algolia": "^1.10.1",
    "@nuxtjs/critters": "^0.6.0",
    "@nuxtjs/fontaine": "^0.4.1",
    "@nuxtjs/google-fonts": "^3.1.3",
    "@nuxtjs/i18n": "8.1.1",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@storefront-ui/typography": "^2.0.0",
    "@storefront-ui/vue": "^2.6.0",
    "@vue-storefront/sdk": "^1.1.2",
    "axios": "^1.6.8",
    "clipboard": "^2.0.11",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "lodash": "^4.17.21",
    "nuxt-delay-hydration": "^1.3.3",
    "nuxt-lazy-hydrate": "^1.0.0",
    "nuxt-lodash": "^2.5.3",
    "object-hash": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.6.2",
    "vue-toastification": "next",
    "vue3-json-viewer": "^2.2.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/client-preset": "^4.0.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-generic-sdk": "^3.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nuxt": "^3.11.2",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "overrides": {
    "react-json-view": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  },
  "resolutions": {
    "consola": "^3.0.0"
  }
}
